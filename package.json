{
  "name": "browser-gnn",
  "version": "0.2.0",
  "description": "BrowserGNN by Dr. Lee - The World's First Comprehensive Graph Neural Network Library for the Browser",
  "author": "Dr. Lee",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "browser-gnn": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "example": "vite examples --port 3333",
    "build:demo": "vite build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "graph-neural-network",
    "gnn",
    "gcn",
    "gat",
    "graphsage",
    "machine-learning",
    "deep-learning",
    "webgpu",
    "webassembly",
    "browser",
    "client-side-ml",
    "privacy-preserving",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fenago/BrowserGNN.git"
  },
  "bugs": {
    "url": "https://github.com/fenago/BrowserGNN/issues"
  },
  "homepage": "https://github.com/fenago/BrowserGNN",
  "dependencies": {
    "onnxruntime-web": "^1.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@webgpu/types": "^0.1.67",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
